    <script>
        // Function to handle claim button clicks
        function handleClaimButtonClick() {
            const email = document.getElementById('email-input').value;
            
            if (email && email.includes('@')) {
                // Randomly select one of the offer URLs
                const offerUrls = [
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=51674&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=68831&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=51675&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=68777&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=69145&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=69339&tracking_id=',
                    'https://singingfiles.com/show.php?l=0&u=2423732&id=68846&tracking_id='
                ];
                
                const randomUrl = offerUrls[Math.floor(Math.random() * offerUrls.length)];
                
                // Open the URL in a new tab
                window.open(randomUrl, '_blank');
            } else {
                alert('Please enter a valid email address to claim your gift!');
            }
        }
        
        // Add event listeners to both claim buttons
        document.getElementById('claim-button').addEventListener('click', handleClaimButtonClick);
        document.getElementById('bottom-claim-button').addEventListener('click', handleClaimButtonClick);
        
        // Make prize cards more interactive with hover effect
        const prizeCards = document.querySelectorAll('.prize-card');
        prizeCards.forEach(card => {
            card.addEventListener('mouseenter', () => {
                card.style.transform = 'translateY(-5px)';
                card.style.borderColor = '#FF6B6B';
                card.style.boxShadow = '0 10px 20px rgba(0, 0, 0, 0.1)';
            });
            
            card.addEventListener('mouseleave', () => {
                card.style.transform = '';
                card.style.borderColor = 'transparent';
                card.style.boxShadow = '';
            });
        });
    </script>
